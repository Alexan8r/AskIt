<h1 class="mb-4"> <%= @question.title %> </h1>
<h2 class="lead my-3"> <%= @question.body %></h2>
<h3 class="mb-4"> <%=Time.now %></h3>

<%=link_to 'Show', questions_path(@question), class: 'btn btn-primary' %>
<%=link_to 'Edit', questions_path(@question), class: 'btn btn-secondary' %>
<%= link_to 'Destroy', question_path(@question), class: 'btn btn-danger',
            data: {method: :delete, confirm: 'Are you sure?'}%>